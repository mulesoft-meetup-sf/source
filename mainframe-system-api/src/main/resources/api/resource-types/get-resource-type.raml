#%RAML 1.0 ResourceType
get:
  description: Get <<type-name>>
  responses:
    200:
      body:
        application/json:
          type: <<type-name>>
          example: <<example>>
    404:
      body:
        application/json:
          properties:
            message:
              type: string
    500:
      body:
        application/json:
          properties:
            message: 
              type: string